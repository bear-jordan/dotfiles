# --- check interactive mode ---

[[ $- != *i* ]] && return
[[ ! -z "$TOOLBOX_PATH" ]] && return

# --- load configs ---

BASH_CONFIG_DIR="$HOME/.config/bash"

source "$BASH_CONFIG_DIR/alias.sh"
source "$BASH_CONFIG_DIR/env.sh"
source "$BASH_CONFIG_DIR/shell-integrations.sh"
source "$BASH_CONFIG_DIR/prompt.sh"
